{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "blog/blogposts.html", "use": "@vercel/static" },
    { "src": "blog/blogstyle.css", "use": "@vercel/static" },
    { "src": "blog/blogposts2.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/blog", "dest": "/blog/blogposts.html" }
  ]
}
